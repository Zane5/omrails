<div class="row">
	<div class="col-xs-6 col-md-offset-4">
		<div class="well">
			<p>
				<%= @pin.description %>
			</p>
			<p>
				<%= @pin.user.name %>
			</p>
			<% if current_user == @pin.user %>
				<%= link_to 'Edit', edit_pin_path(@pin) %> |
			<% end %>
			<%= link_to 'Back', pins_path %>
			
		</div>
	</div>
</div>


